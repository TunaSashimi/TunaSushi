            <flag name="center_horizontal" value="0x01" />
            <flag name="right" value="0x02" />
            <flag name="top" value="0x00" />
            <flag name="center_vertical" value="0x04" />
            <flag name="bottom" value="0x08" />
            <flag name="center" value="0x05" />
        </attr>

        <attr name="srcAnchorNormal" format="reference" />
        <attr name="srcAnchorNormalWidth" format="dimension" />
        <attr name="srcAnchorNormalHeight" format="dimension" />
        <attr name="srcAnchorNormalDx" format="dimension" />
        <attr name="srcAnchorNormalDy" format="dimension" />


        <attr name="rippleCircleColorEndOuter" format="reference|color" />
        <attr name="rippleMode">
            <!-- In the animation begins and ends where the rate of change is relatively slow, accelerate the time in the middle -->
            <enum name="accelerateDecelerateInterpolator" value="0" />
            <!-- Animation begins at the local rate of change is relatively slow, and then began to accelerate -->
            <enum name="accelerateInterpolator" value="1" />
            <!-- Initially thrown forward and then backward -->
            <enum name="anticipateInterpolator" value="2" />
            <!-- At the beginning of the return value after a certain value thrown forward and then backward -->
            <enum name="anticipateOvershootInterpolator" value="3" />
            <!-- When the animation ends bounce -->
            <enum name="bounceInterpolator" value="4" />
            <!-- The number of times a particular animation loop, velocity change along a sine curve -->
            <enum name="cycleInterpolator" value="5" />
            <!-- Fast and slow at the beginning of the local animation -->
            <enum name="decelerateInterpolator" value="6" />
            <!-- In a constant rate of change -->
            <enum name="linearInterpolator" value="7" />
            <!-- Thrown forward certain value and then return to its original position -->
            <enum name="overshootInterpolator" value="8" />
        </attr>
    </declare-styleable>


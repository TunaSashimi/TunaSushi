            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
            <enum name="bold_italic" value="3" />
        </attr>

        <attr name="textDx" format="dimension" />
        <attr name="textDy" format="dimension" />
        <attr name="textFractionDx" format="fraction" />
        <attr name="textFractionDy" format="fraction" />

        <attr name="textShadowRadius" format="dimension" />
        <attr name="textShadowColor" format="reference|color" />
        <attr name="textShadowDx" format="dimension" />
        <attr name="textShadowDy" format="dimension" />

        <attr name="textMark" format="boolean" />
        <attr name="textMarkTouchable" format="boolean" />
        <attr name="textMarkRadius" format="dimension" />
        <attr name="textMarkColor" format="reference|color" />
        <attr name="textMarkTextValue" format="string" />
        <attr name="textMarkTextSize" format="dimension" />
        <attr name="textMarkTextColor" format="reference|color" />
        <attr name="textMarkDx" format="dimension" />
        <attr name="textMarkDy" format="dimension" />
        <attr name="textMarkFractionDx" format="fraction" />
        <attr name="textMarkFractionDy" format="fraction" />

        <attr name="contentValue" format="string" />
        <attr name="contentSize" format="dimension" />
        <attr name="contentColorNormal" format="reference|color" />
        <attr name="contentColorPress" format="reference|color" />
        <attr name="contentColorSelect" format="reference|color" />
        <attr name="contentPaddingLeft" format="dimension" />
        <attr name="contentPaddingRight" format="dimension" />
        <attr name="contentRowSpaceRatio" format="fraction" />
        <attr name="contentGravity">
            <enum name="all_center" value="0" />
            <enum name="all_left" value="1" />
            <enum name="center_left" value="2" />
            <enum name="left_center" value="3" />
        </attr>
        <attr name="contentTypeFace">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
            <enum name="bold_italic" value="3" />
        </attr>

        <attr name="contentDx" format="dimension" />
        <attr name="contentDy" format="dimension" />
        <attr name="contentFractionDx" format="fraction" />
        <attr name="contentFractionDy" format="fraction" />

        <attr name="contentShadowRadius" format="dimension" />
        <attr name="contentShadowColor" format="reference|color" />
        <attr name="contentShadowDx" format="dimension" />
        <attr name="contentShadowDy" format="dimension" />

        <attr name="contentMark" format="boolean" />
        <attr name="contentMarkTouchable" format="boolean" />
        <attr name="contentMarkRadius" format="dimension" />
        <attr name="contentMarkColor" format="reference|color" />
        <attr name="contentMarkTextValue" format="string" />
        <attr name="contentMarkTextSize" format="dimension" />
        <attr name="contentMarkTextColor" format="reference|color" />
        <attr name="contentMarkDx" format="dimension" />
        <attr name="contentMarkDy" format="dimension" />
        <attr name="contentMarkFractionDx" format="fraction" />
        <attr name="contentMarkFractionDy" format="fraction" />

        <attr name="srcLeft" format="reference" />
        <attr name="srcLeftWidth" format="dimension" />
        <attr name="srcLeftHeight" format="dimension" />

        <!-- tunaTextPaddingLeft means distance between tunaSrcLeft and The leftmost,note about the tunaSrcLeftPadding -->
        <attr name="srcLeftPadding" format="dimension" />
        <attr name="srcLeftDx" format="dimension" />
        <attr name="srcLeftDy" format="dimension" />

        <attr name="srcRight" format="reference" />
        <attr name="srcRightWidth" format="dimension" />
        <attr name="srcRightHeight" format="dimension" />

        <!-- tunaTextPaddingRight means distance between tunaSrcRight and The leftmost,note about the tunaSrcRightPadding -->
        <attr name="srcRightPadding" format="dimension" />
        <attr name="srcRightDx" format="dimension" />
        <attr name="srcRightDy" format="dimension" />

        <attr name="porterDuffXfermode">
            <enum name="src_in" value="0" />
            <enum name="src_out" value="1" />
        </attr>

        <attr name="material">
            <enum name="spread" value="0" />
            <enum name="trans" value="1" />
        </attr>
        <attr name="materialMove" format="boolean" />
        <attr name="materialDuraction" format="integer" />

        <attr name="materialTimeInterpolator">
            <!-- In the animation begins and ends where the rate of change is relatively slow, accelerate the time in the middle -->
            <enum name="accelerateDecelerateInterpolator" value="0" />
            <!-- Animation begins at the local rate of change is relatively slow, and then began to accelerate -->
            <enum name="accelerateInterpolator" value="1" />
            <!-- Initially thrown forward and then backward -->
            <enum name="anticipateInterpolator" value="2" />
            <!-- At the beginning of the return value after a certain value thrown forward and then backward -->
            <enum name="anticipateOvershootInterpolator" value="3" />
            <!-- When the animation ends bounce -->
            <enum name="bounceInterpolator" value="4" />
            <!-- The number of times a particular animation loop, velocity change along a sine curve -->
            <enum name="cycleInterpolator" value="5" />
            <!-- Fast and slow at the beginning of the local animation -->
            <enum name="decelerateInterpolator" value="6" />
            <!-- In a constant rate of change -->
            <enum name="linearInterpolator" value="7" />
            <!-- Thrown forward certain value and then return to its original position -->
            <enum name="overshootInterpolator" value="8" />
        </attr>

    </declare-styleable>


        <attr name="press" format="boolean" />
        <attr name="select" format="boolean" />
        <attr name="selectType">
            <enum name="none" value="0" />
            <enum name="reverse" value="2" />
            <enum name="same" value="1" />
        </attr>
        <attr name="rotate" format="integer" />
        <attr name="animationable" format="boolean" />
        <attr name="backgroundNormal" format="reference|color" />
        <attr name="backgroundPress" format="reference|color" />

        <attr name="backgroundPressShadowDx" format="dimension" />
        <attr name="backgroundPressShadowDy" format="dimension" />
        <attr name="backgroundSelectShadowRadius" format="dimension" />
        <attr name="backgroundSelectShadowColor" format="reference|color" />
        <attr name="backgroundSelectShadowDx" format="dimension" />
        <attr name="backgroundSelectShadowDy" format="dimension" />

        <attr name="srcNormal" format="reference" />
        <attr name="srcPress" format="reference" />
        <attr name="srcSelect" format="reference" />


<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#c1eaee">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG01"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_margin="16dp"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG02"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignTop="@id/tSVG01"
            android:layout_toRightOf="@id/tSVG01"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:porterDuffXfermode="src_out" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG03"
            android:layout_width="100dp"
            android:layout_height="60dp"
            android:layout_alignTop="@id/tSVG02"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@id/tSVG02"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="circle"
            app:porterDuffXfermode="src_in" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG04"
            android:layout_width="100dp"
            android:layout_height="60dp"
            android:layout_alignTop="@id/tSVG03"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@id/tSVG03"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="circle"
            app:porterDuffXfermode="src_out" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG05"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_below="@id/tSVG01"
            android:layout_alignLeft="@id/tSVG01"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="star" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG06"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignTop="@id/tSVG05"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@id/tSVG05"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="star"
            app:porterDuffXfermode="src_out" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG07"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignTop="@id/tSVG06"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@id/tSVG06"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="heart" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG08"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignTop="@id/tSVG07"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@id/tSVG07"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="heart"
            app:porterDuffXfermode="src_out" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG09"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_below="@id/tSVG05"
            android:layout_alignLeft="@id/tSVG05"
            android:layout_marginTop="16dp"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="flower" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG10"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignTop="@id/tSVG09"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@id/tSVG09"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="flower"
            app:porterDuffXfermode="src_out" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG11"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignTop="@id/tSVG10"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@id/tSVG10"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="pentagon" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG12"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignTop="@id/tSVG11"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@id/tSVG11"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="pentagon"
            app:porterDuffXfermode="src_out" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG13"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_below="@id/tSVG09"
            android:layout_alignLeft="@id/tSVG09"
            android:layout_marginTop="16dp"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="sixteenedge" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG14"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignTop="@id/tSVG13"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@id/tSVG13"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="sixteenedge"
            app:porterDuffXfermode="src_out" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG15"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignTop="@id/tSVG14"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@id/tSVG14"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="fortyedge" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG16"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignTop="@id/tSVG15"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@id/tSVG15"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="fortyedge"
            app:porterDuffXfermode="src_out" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG17"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_below="@id/tSVG13"
            android:layout_alignLeft="@id/tSVG13"
            android:layout_marginTop="16dp"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="snail" />

        <com.tunasushi.tuna.TSVG
            android:id="@+id/tSVG18"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignTop="@id/tSVG17"
            android:layout_marginLeft="16dp"
            android:layout_toRightOf="@id/tSVG17"
            app:SVGSrc="@drawable/bitmap_icon_tunasashimi"
            app:SVGType="snail"
            app:porterDuffXfermode="src_out" />

    </RelativeLayout>
</ScrollView>